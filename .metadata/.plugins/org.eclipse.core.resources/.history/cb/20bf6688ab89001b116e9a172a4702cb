#include<iostream>
#include<vector>
#include <algorithm>
using namespace std;
int main(){
	int n;
	cin >> n;
	vector<pair<int,int>> m;
	for( int i = 0; i < n; i++) {
		pair<int,int> p;
		int ma,f;
		cin >> ma >> f;
		p.second = f;
		p.first = ma;
		m.push_back(p);
	}
	sort(m.begin(), m.end());
	int mk = m[0].first;

	cout << mk;



}
